{
  "name": "learnrn",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios:dev": "react-native run-ios --scheme LearnRNDev --configuration DebugDev",
    "ios:staging" : "react-native run-ios --scheme LearnRNStaging --configuration DebugDev",
    "ios:prod" : "react-native run-ios --scheme LearnRNProd --configuration DebugDev",
    "android:dev": "react-native run-android --no-jetifier --variant=devDebug --appId 'com.demoenv.app.dev'",
    "android:staging": "react-native run-android --no-jetifier --variant=stagingDebug --appId 'com.demoenv.app.stg'",
    "android:prod": "react-native run-android --no-jetifier --variant=productDebug --appId 'com.demoenv.app.prod'",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "re-package": "npx react-native-rename learnrn -b net.aedev.learn",
    "prepare": "husky install"
  },
  "dependencies": {
    "husky": "^8.0.3",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-config": "^1.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
