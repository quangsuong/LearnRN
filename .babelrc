{
  "env": {
    "production": {
      "plugins": [
        "transform-remove-console"
      ]
    }
  },
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./"
        ],
        "alias": {
          "@assets": "./src/app/assets",
          "@common": "./src/app/common",
          "@foundation": "./src/app/library/foundation",
          "@hooks": "./src/app/common/hooks",
          "@validate": "./src/app/common/yup-validate",
          "@native-module": "./src/app/common/native-module",
          "@listener": "./src/app/common/redux/listener.ts",
          "@firebase": "./src/app/common/firebase",
          "@animated": "./src/app/common/animated",
          "@config": "./src/app/config",
          "@features": "./src/app/features",
          "@components": "./src/app/library/components",
          "@networking": "./src/app/library/networking",
          "@utils": "./src/app/library/utils",
          "@storage": "./src/app/library/utils/storage",
          "@model": "./src/app/model",
          "@navigation": "./src/app/navigation",
          "@redux": "./src/app/redux",
          "@theme": "./src/app/themes",
          "@transition": "./src/app/transition",
          "@customHooks": "./src/app/hooks",
          "@fakeData": "./src/app/fake-data",
          "@service": "./src/app/htc-service"
        }
      }
    ]
  ]
}
